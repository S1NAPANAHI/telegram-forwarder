{
  "name": "backend",
  "version": "1.0.0",
  "description": "Telegram Forwarder Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prebuild": "node deploy-check.js",
    "build": "npm install && npm audit fix --audit-level moderate",
    "postbuild": "echo 'Build completed successfully'",
    "test": "node deploy-check.js && echo 'Health check passed'",
    "health-check": "node deploy-check.js",
    "audit-fix": "npm audit fix --force"
  },
  "keywords": ["telegram", "bot", "forwarder", "api"],
  "author": "SINA PANAHI",
  "license": "ISC",
  "type": "commonjs",
  "engines": {
    "node": "20.x",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.75.0",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "natural": "^8.1.0",
    "node-cron": "^4.2.1",
    "node-telegram-bot-api": "^0.66.0",
    "persian-preprocess": "^1.0.0",
    "puppeteer": "^24.24.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "input": "^1.0.1",
    "telegram": "^2.25.15",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
